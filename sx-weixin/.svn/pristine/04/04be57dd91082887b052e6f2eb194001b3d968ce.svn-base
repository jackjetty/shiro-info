@charset "utf-8";
/* CSS Document */
*{
	padding:0;
	margin:0;
}
a{
	text-decoration:none;
}
li{
	list-style:none;
}
body{
	background-color:#f5f5f9;
}
.page{
	padding:0 15px;
}
.main{
	padding:20px 0;
}
.main .vip{
	overflow:hidden;
	vertical-align:middle;
	margin:50px auto 0 auto;
}
.main .vip img{
	display:block;
	width:100px;
	margin:0 auto;
}
.main .vip  span{
	display:block;
	text-align:center;
	color:#b5b5b5;
	line-height:40px;
	font-size:14px;
	font-family:'微软雅黑';
}
.page  a{
	display:block;
	width:120px;
	margin:0 auto;
	color:white;
	line-height:40px;
	font-size:16px;
	font-family:'微软雅黑';
	text-align:center;
	background-color:#e52222;
	border-radius:3px;
	-o-border-radius:3px;
	-moz-border-radius:3px;
	-ms-border-radius:3px;
	-webkit-border-radius:3px;
}