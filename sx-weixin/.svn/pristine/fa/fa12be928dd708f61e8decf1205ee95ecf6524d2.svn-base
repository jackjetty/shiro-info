*{
	padding:0;
	margin:0;
}
body{
	background-color:#EBEEEF;
}
.focus {
	width:100%;
	position: relative;
	overflow: hidden;
	display: inline-block;
}
.focus .hd {
	width: 100%;
	height: 15px;
	position: absolute;
	z-index: 1;
	bottom:12px;
	text-align: center;
}
.focus .hd ul {
	display: inline-block;
	padding: 4px 10px;
	background-color: rgba(255,255,255,0.7);
	-webkit-border-radius: 25px;
	-moz-border-radius: 25px;
	border-radius: 25px;
	font-size: 0;
	vertical-align: top;
}
.focus .hd ul li {
	display: inline-block;
	width: 6px;
	height: 6px;
	-webkit-border-radius: 50%;
	-moz-border-radius: 50%;
	border-radius: 50%;
	background-color: rgba(0,0,0,0.2);
	margin: 0 5px;
	vertical-align: top;
	overflow: hidden;
}
.focus .hd ul .on {
	background: #00B6F0;
}
.focus .bd {
	position: relative;
	z-index: 0;
}
.focus .bd li {
}
.focus .bd li img {
	width: 100%;
	height:160px;
	display:block;
	
}
.focus .bd li a {
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0); /* »°œ˚¡¥Ω”∏ﬂ¡¡ */
}
