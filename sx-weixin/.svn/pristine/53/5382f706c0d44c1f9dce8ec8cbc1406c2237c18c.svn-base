@charset "utf-8";
/* CSS Document */
*{
	padding:0;
	margin:0;
}
a{
	text-decoration:none;
}
li{
	box1-style:none;
}
body{
	background-color:#EBEEEF;
}
.page{
}
.page .information{
}
.page .information .box{
	background-color:white;
	border-top:1px solid #e1e1e1;
	border-bottom:1px solid #e1e1e1;
}
.page .information .box .box1{
	border-bottom:1px solid #e1e1e1;
	overflow:hidden;
	padding:8px 10px 8px 0;
	margin-left:10px;
}
.page .information .box1 span{
	color:#333333;
	line-height:32px;
	font-size:14px;
	font-family:'微软雅黑';
	float:left;
	display:block;
	font-weight:bold;
}
.page .information .box1  input{
	width:60%;
	float:right;
	height:32px;
	border:0;
	box-sizing:border-box;
	background-color:transparent;
	-webkit-appearance: none;
	color:#757575;
	line-height:32px;
	font-size:14px;
	font-family:'微软雅黑';
	text-align:right;
	-webkit-tap-highlight-color:transparent;
}
.page .main{
	padding:5px 0;
}
.page .main .box{
	margin:8px 0;
}
	
.page .main .box h1{
	font-weight:normal;
}
.page .main .box h1 .title{
	overflow:hidden;
	padding:5px 15px;
}
.page .main .box h1 .title img{
	float:left;
	width:22px;
	height:22px;
	display:block;
}
.page .main .box h1  b{
	float:left;
	font-size:12px;
	font-family:'微软雅黑';
	line-height:22px;
	color:#292E35;
	margin-left:8px;
}

.page .main .box h1  span{
	padding:0 15px 0 24px;
	font-size:12px;
	font-family:'微软雅黑';
	line-height:22px;
	color:#747474;
	display:block;
	text-indent:2em;
}
.photo{
	border-top:1px solid #e1e1e1;
	border-bottom:1px solid #e1e1e1;
	padding:10px 15px;
	background-color:white;
	margin:8px 0px 16px 0px;
}
.photo .upload .add{
	padding:20px 0;
	border:1px dashed #e1e1e1;
	border-radius:3px;
	-o-border-radius:3px;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	margin:16px 0 8px 0;
}
.photo .upload .add .addIcon{
	width:66px;
	height:66px;
	display:block;
	margin:0 auto;
}
.photo .upload .add span{
	display:block;
	line-height:20px; 
	text-align:center; 
	color:#999999;
	font-size:12px;
	font-family:“微软雅黑”; 
	margin-top:10px;
}
.photo .upload b{
	display:block;
	line-height:25px; 
	text-align:center; 
	color:#E89E10;
	font-size:12px;
	font-family:“微软雅黑”; 
	font-weight:normal;
}
.photo .upload .car{
	width:100%;
	max-height:140px;
	display:block;
	border-radius:3px;
	-o-border-radius:3px;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	margin:8px 0;
}
footer{
	padding:20px 0;
}
footer .copyright{
	overflow:hidden;
	width:245px;
	margin:0 auto;
}
footer .logo{
	float:left;
	width:32px; 
	height:32px;
	display:block;
}
footer span{ 
	float:right;
	width:206px;
	line-height:32px; 
	text-align:right; 
	color:#96a4ae;
	font-size:12px;
	font-family:“微软雅黑”; 
}



.more-box{
	width:100%;
	max-height:145px;
	position: relative;
	border:0px;
	margin:5px 0px;
}
.more-box .clear{
	width:20px;
	height:20px;
	position:absolute;
	top:5px; 
	right:5px; 
	border:0px;
}
.more-box .clear img{
	width:20px;
	height:20px;
	border:0px; 
}
 
